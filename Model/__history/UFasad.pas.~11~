unit UFasad;

interface

uses UStore;

type
  IFasad = interface
    procedure getStore();
  end;

  TFasad = class(TInterfacedObject, IFasad)
  private
    store: TStore;
  public
    procedure getStore();
    constructor Create();
  end;

implementation

{ TFasad }

constructor TFasad.Create;
begin

end;

procedure TFasad.getStore;
begin
  store:=TStore.Create;
end;

end.

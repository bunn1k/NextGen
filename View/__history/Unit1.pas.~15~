unit Unit1;

interface

uses
   UItemID, UMoney, Vcl.StdCtrls, System.Classes, Vcl.Controls;

type
  TForm1 = class(TForm)
    Edit1: TEdit;
    Edit2: TEdit;
    Button1: TButton;
    procedure FormCreate(Sender: TObject);
    procedure Button1Click(Sender: TObject);
  private
    /// <link>aggregation</link>
    fasad: IFasad;
  public
    { Public declarations }
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
begin
  Form2.Show;
end;

procedure TForm1.FormCreate(Sender: TObject);
begin
  fasad:=TFasad.Create();
end;

end.
